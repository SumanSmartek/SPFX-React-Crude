{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "list-crude-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/listCrude/ListCrudeWebPart.js",
          "manifest": "./src/webparts/listCrude/ListCrudeWebPart.manifest.json"
        }
      ]
    },
    "library-crude-web-part": {
      "components": [
